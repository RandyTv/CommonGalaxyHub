game:GetService("StarterGui"):SetCore("SendNotification",{ Title = "Notification"; Text = "Made by Clover King#6209"; })
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Common Galaxy Hub", "Midnight")
local Main = Window:NewTab("Home")
local MainSection = Main:NewSection("Credits")
MainSection:NewLabel("Clover King#6209")
local Script = Window:NewTab("Script's")
local ScriptSection = Script:NewSection("Common Script's")
ScriptSection:NewSlider("Walk Speed", "Walk Speed info", 500, 0, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
ScriptSection:NewSlider("Jump Power", "Jump Power info", 500, 1, function(jp)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
end)
ScriptSection:NewTextBox("Teleport To Player", "Teleports To Player", function(tp)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[tp].Character.HumanoidRootPart.CFrame
    local Victim = tp
end)
ScriptSection:NewLabel("Teleport Only Works If Full User And Not Using Fe")
ScriptSection:NewLabel("Animation.")
ScriptSection:NewButton("Fling All", "Fling all Players", function()
loadstring(game:HttpGet('https://pastebin.com/raw/9uJC1ay7'))()
end)
ScriptSection:NewLabel("Flings all Players forever")
ScriptSection:NewLabel("Warning It May Cause Lag!")
ScriptSection:NewButton("Fly ", "Fly For Pc", function()
loadstring(game:HttpGet('https://pastebin.com/raw/Labz1cM3'))()
end)
ScriptSection:NewButton("KeyBoard ", "KeyBoard for Mobile", function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/manimcool21/Keyboard-FE/main/Protected%20(3).lua'),true))()
end)
ScriptSection:NewLabel("Use the keyboard to move in fly Script Mobiles!") 
